{
  "name": "vinsliu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch-css": "sass --watch src/assets/scss/main.scss:src/assets/css/style.css",
    "dev": "vite",
    "build-css": "sass src/assets/scss/main.scss src/assets/css/style.css --no-source-map --style=compressed",
    "build": "npm run build-css && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build && cp dist/index.html dist/404.html",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "sass": "^1.93.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
